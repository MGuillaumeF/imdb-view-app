{
  "name": "imdb-view-app",
  "author": {
    "name": "MGuillaumeF",
    "email": "michel.guillaume--foucaud@hotmail.fr"
  },
  "description": "IMDP application to display movies tickets",
  "version": "0.1.0",
  "productName": "IMDB View",
  "homepage": "https://mguillaumef.github.io/imdb-view-app",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {},
  "scripts": {
    "deploy": "npm run build&&npm run build-doc && mv docs build/docs && npm run gh-pages",
    "gh-pages": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "el:build": "PUBLIC_URL=./ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:doc": "npx styleguidist build",
    "dev-doc": "npx styleguidist server",
    "cy:open": "cypress open",
    "cy:run": "wait-on http://localhost:3000/ && cypress run --config-file cypress.json",
    "prettier": "prettier --write . --ignore-path ./.gitignore",
    "lint:eslint": "eslint . --fix --format html --output-file ./dist/reports/lint/eslint-report.html --ignore-path ./.gitignore",
    "lint:eslint:json": "eslint . --fix --format json --output-file ./dist/reports/lint/eslint-report.json --ignore-path ./.gitignore",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --custom-syntax postcss-scss --fix --formatter json > ./dist/reports/lint/stylelint-report.json --ignore-path ./.gitignore",
    "test": "npm-run-all -p start cy:run",
    "vscode:prepublish": "npm start & npm run cy:run",
    "el:dev": "wait-on http://locahost:3000 && npm run el:run",
    "el:run": "electron public/electron.js",
    "el:package": "electron-builder",
    "el:installer-mac": "electron-installer-dmg ./release-builds/imdb-view-app-darwin-x64/imdb-view-app.app imdb-view-app --out=installer --overwrite --icon=/Users/michelguillaume/Documents/Michel/code/imdb-view-app/src/icons/heart.png"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "com.$npm_package_name.app",
    "productName": "IMDB View",
    "copyright": "Copyright Â© 2020 ${author}",
    "mac": {
      "category": "public.app.category.utilities"
    },
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
       "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-transform-react-constant-elements": "^7.17.6",
    "@babel/plugin-transform-regenerator": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cypress/code-coverage": "^3.9.12",
    "acorn": "^6.4.2",
    "cypress": "^9.6.0",
    "cypress-movie": "^3.0.20",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-sonarqube-reporter": "^1.10.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-react": "^0.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.1.0",
    "electron-winstaller": "^4.0.1",
    "gh-pages": "^3.1.0",
    "react-docgen-typescript": "^1.20.5",
    "react-styleguidist": "^11.1.1",
    "wait-on": "^5.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.21.2",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "@mguillaumef/audiso": "^0.0.4",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-prettier": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.3",
    "react-scripts": "^5.0.1",
    "typescript": "^4.6.4"
  }
}
